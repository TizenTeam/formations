<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <welcome-file-list>
    <welcome-file>home</welcome-file>
  </welcome-file-list>
  <filter>
    <display-name>SecuriteFilter</display-name>
    <filter-name>SecuriteFilter</filter-name>
    <filter-class>fr.lteconsulting.SecuriteFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>SecuriteFilter</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
  <servlet>
    <servlet-name>HomeServlet</servlet-name>
    <servlet-class>fr.lteconsulting.servlet.vue.HomeServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>HomeServlet</servlet-name>
    <url-pattern>/home</url-pattern>
  </servlet-mapping>
  <session-config>
    <session-timeout>30</session-timeout>
  </session-config>
  <servlet>
    <description></description>
    <display-name>CartesServlet</display-name>
    <servlet-name>CartesServlet</servlet-name>
    <servlet-class>fr.lteconsulting.servlet.vue.ListeCartesServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>CartesServlet</servlet-name>
    <url-pattern>/cartes</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>LoginServlet</display-name>
    <servlet-name>LoginServlet</servlet-name>
    <servlet-class>fr.lteconsulting.servlet.action.LoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LoginServlet</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>LogoutServlet</display-name>
    <servlet-name>LogoutServlet</servlet-name>
    <servlet-class>fr.lteconsulting.servlet.action.LogoutServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LogoutServlet</servlet-name>
    <url-pattern>/logout</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>DeleteCarteServlet</display-name>
    <servlet-name>DeleteCarteServlet</servlet-name>
    <servlet-class>fr.lteconsulting.servlet.action.DeleteCarteServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>DeleteCarteServlet</servlet-name>
    <url-pattern>/deleteCarte</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>EditCarteServlet</display-name>
    <servlet-name>EditCarteServlet</servlet-name>
    <servlet-class>fr.lteconsulting.servlet.action.EditCarteServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>EditCarteServlet</servlet-name>
    <url-pattern>/editCarte</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>ToggleCarteServlet</display-name>
    <servlet-name>ToggleCarteServlet</servlet-name>
    <servlet-class>fr.lteconsulting.servlet.action.ToggleCarteServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ToggleCarteServlet</servlet-name>
    <url-pattern>/toggleCarte</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>MainServlet</display-name>
    <servlet-name>MainServlet</servlet-name>
    <servlet-class>fr.lteconsulting.servlet.vue.MainServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>MainServlet</servlet-name>
    <url-pattern>/main</url-pattern>
  </servlet-mapping>
  <listener>
    <listener-class>fr.lteconsulting.ApplicationDataInitializationListener</listener-class>
  </listener>
</web-app>